{
    "name": "dgame/php-constant",
    "description": "php constant",
    "license": "MIT",
    "type": "library",
    "homepage": "https://github.com/php-constant",
    "keywords": [
        "Constant",
        "Immutable"
    ],
    "authors": [
        {
            "name": "Randy Schuett ",
            "email": "rswhite4@gmail.com",
            "homepage": "https://github.com/php-constant"
        }
    ],
    "require": {
        "php": ">=7.0"
    },
    "autoload": {
        "classmap": [
            "src/"
        ],
        "files": [
            "src/functions.php"
        ],
        "psr-4": {
            "Dgame\\Constant\\": "src/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "5.*.*",
        "dgame/php-ensurance": "*"
    },
    "autoload-dev": {
        "classmap": [
            "tests/"
        ]
    }
}